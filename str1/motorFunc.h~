#include <stdio.h>
#include <iostream>
#include <cstdlib>
#include <unistd.h>
#include <errno.h>
#include <sys/types.h>
#include <sys/time.h>
#include <fcntl.h>

using namespace std;

int forward(char * file) {
  int handle = open(file, O_WRONLY);
  write(handle, '1', 1);
  sleep(5);
  close(handle);
}
